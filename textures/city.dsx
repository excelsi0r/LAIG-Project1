        </primitive>
        <primitive id="sphereZep">
            <sphere radius="1" slices="20" stacks="10" />
        </primitive>
        <primitive id="cili">
            <cylinder base="0.5" top="0.5" height="3" slices="20" stacks="10" />
        </primitive>
        <primitive id="tria">
            <triangle x1="-1" y1="0" z1="1" x2="1" y2="0" z2="1" x3="0" y3="2" z3="0" />
        </primitive>

    </primitives>


